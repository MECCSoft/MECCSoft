<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					User Name:
				</td>
				<td>
					<g:TextBox ui:field="txtUserName"></g:TextBox>
				</td>
			</tr>
			<tr>
				<td>
					Password:
				</td>
				<td>
					<g:PasswordTextBox ui:field="txtPassword"></g:PasswordTextBox>
				</td>
			</tr>
		</table>
		<g:Label ui:field="lblResult"></g:Label>
		<g:Button ui:field="btnSignup">Sign-up</g:Button>
		<g:Button ui:field="btnLogin">Login</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 